{
  "expo": {
    "name": "FitNudge",
    "slug": "fitnudge",
    "scheme": "fitnudge",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "experiments": {
      "autolinkingModuleResolution": true
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.fitnudge.app",
      "buildNumber": "1",

      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "com.apple.security.application-groups": ["group.com.fitnudge.app"]
      },
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "remote-notification"],
        "ITSAppUsesNonExemptEncryption": false,
        "NSSupportsLiveActivities": true,
        "NSLocalNetworkUsageDescription": "We need access to your local network to enable live activities for your habits and goals.",
        "NSCameraUsageDescription": "We need access to your camera to take progress photos for your habits and goals.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to select progress photos for your habits and goals.",
        "NSPhotoLibraryAddUsageDescription": "We need access to save progress photos to your photo library.",
        "NSMicrophoneUsageDescription": "We need access to your microphone to enable voice input for AI chat and habits and goals coaching."
      },
      "associatedDomains": ["applinks:fitnudge.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "package": "com.fitnudge.app",
      "versionCode": 1,
      "permissions": [
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "fitnudge.app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "posthogApiKey": "phc_mlphAT3brUuEDVqCQyxLNUp4c2qRUIIofXzyGxksdAQ",
      "posthogHost": "https://us.i.posthog.com",
      "blogUrl": "https://fitnudge.app/blog",
      "router": {},
      "eas": {
        "projectId": "c3c6f292-99b6-4d63-8e88-ee8c94048e19",
        "build": {}
      }
    },
    "owner": "fitnudge",
    "plugins": [
      [
        "@bacons/apple-targets",
        {
          "appleTeamId": "AUBNC6893W"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.google.android.gms.** { *; }"
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "17.2",
            "buildReactNativeFromSource": true
          }
        }
      ],
      "expo-apple-authentication",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "../../packages/assets/fonts/Nunito-Regular.ttf",
            "../../packages/assets/fonts/Nunito-Medium.ttf",
