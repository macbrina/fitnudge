{
  "expo": {
    "name": "FitNudge",
    "slug": "fitnudge",
    "scheme": "fitnudge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.fitnudge.app",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "remote-notification"],
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:fitnudge.app", "applinks:fitnudge.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fitnudge.app",
      "permissions": [
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "fitnudge.app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "posthogApiKey": "phc_mlphAT3brUuEDVqCQyxLNUp4c2qRUIIofXzyGxksdAQ",
      "posthogHost": "https://us.i.posthog.com",
      "router": {},
      "eas": {
        "projectId": "c3c6f292-99b6-4d63-8e88-ee8c94048e19"
      }
    },
    "owner": "fitnudge",
    "plugins": [
      "expo-apple-authentication",
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "../../packages/assets/fonts/Nunito-Regular.ttf",
            "../../packages/assets/fonts/Nunito-Medium.ttf",
            "../../packages/assets/fonts/Nunito-SemiBold.ttf",
            "../../packages/assets/fonts/Nunito-Bold.ttf",
            "../../packages/assets/fonts/Nunito-ExtraBold.ttf",
            "../../packages/assets/fonts/Nunito-RegularItalic.ttf",
            "../../packages/assets/fonts/Nunito-MediumItalic.ttf",
            "../../packages/assets/fonts/Nunito-SemiBoldItalic.ttf",
            "../../packages/assets/fonts/Nunito-BoldItalic.ttf",
            "../../packages/assets/fonts/Nunito-ExtraBoldItalic.ttf",
            "../../packages/assets/fonts/SpaceGrotesk-Regular.ttf",
            "../../packages/assets/fonts/SpaceGrotesk-Medium.ttf",
            "../../packages/assets/fonts/SpaceGrotesk-SemiBold.ttf",
            "../../packages/assets/fonts/SpaceGrotesk-Bold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Nunito",
                "fontDefinitions": [
                  {
                    "path": "../../packages/assets/fonts/Nunito-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-ExtraBold.ttf",
                    "weight": 800
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-RegularItalic.ttf",
                    "weight": 400,
                    "style": "italic"
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-MediumItalic.ttf",
                    "weight": 500,
                    "style": "italic"
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-SemiBoldItalic.ttf",
                    "weight": 600,
                    "style": "italic"
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-BoldItalic.ttf",
                    "weight": 700,
                    "style": "italic"
                  },
                  {
                    "path": "../../packages/assets/fonts/Nunito-ExtraBoldItalic.ttf",
                    "weight": 800,
                    "style": "italic"
                  }
                ]
              },
              {
                "fontFamily": "SpaceGrotesk",
                "fontDefinitions": [
                  {
                    "path": "../../packages/assets/fonts/SpaceGrotesk-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "../../packages/assets/fonts/SpaceGrotesk-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "../../packages/assets/fonts/SpaceGrotesk-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "../../packages/assets/fonts/SpaceGrotesk-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "fitnudge",
          "organization": "fitnudge"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2563EB",
          "sounds": ["./assets/sounds/motivation_call.mp3"],
          "mode": "production"
        }
      ],
      "expo-web-browser",
      "expo-localization"
    ]
  }
}
