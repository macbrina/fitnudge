{
  "name": "@fitnudge/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./src/*.tsx",
    "./styles/*": "./src/styles/*",
    "./lib/utils": {
      "import": "./dist/lib/utils.js",
      "require": "./dist/lib/utils.js",
      "types": "./dist/lib/utils.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@fitnudge/eslint-config": "workspace:*",
    "@fitnudge/tsconfig": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "eslint": "^9.34.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
